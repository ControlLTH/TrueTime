\newcommand*\bibliography@common{%
  \let\rm\upshape \let\titlefont=\slshape \let\authorfont=\upshape
  \def\and{\egroup and \bgroup\authorfont}%
  \let\bf\bfseries
  \bibliographyHook
}

\newcommand*\bibliographyHook{}

\newcommand*\bibset@parameters{%
  \pretolerance\@m \tolerance2000
  \hbadness=\tolerance \emergencystretch=1em
  \sfcode`\.=\@m}

\@ifundefined{internal@bibliography}
   {\let\internal@bibliography\bibliography}
   {}
\renewcommand*\bibliography{%
  \@ifstar
    {\internal@bibliography}%
    {\@ifundefined{thechapter}{%
        \renewcommand*{\BibliographyTitle}{\section{\refname}}%
     }{ %else
        \renewcommand*{\BibliographyTitle}{\chapter{\bibname}}%
     }%
     \internal@bibliography}%
    }

\@ifundefined{internal@bibliographystyle}{%
  \let\internal@bibliographystyle\bibliographystyle}{}
\renewcommand*\bibliographystyle[1]{%
  \@ifundefined{bib@#1}{}{\@nameuse{bib@#1}}%
  \internal@bibliographystyle{#1}}

\newcommand*\bib@LongLabels{\let\the@bibliography\unnumbered@bibliography
  \@ifundefined{etal}{\def\etal{\textsl{et~al.}}}{}
  \def\citeauthoryear##1##2{##1, ##2}
  \def\@citedelim{;}
  \def\@cite##1##2{[{##1\ifx?##2?\else , ##2\fi}]}
}
\newcommand*\bib@Harvard{\bib@LongLabels
  \def\citeauthoryear##1##2{##1~(##2)}
  \def\@cite##1##2{{##1\ifx?##2?\else , ##2\fi}}
}
\let\bib@PublicationList=\bib@LongLabels
\def\@citedelim{,}
\def\@citex[#1]#2{%
  \let\@citea\@empty
  \@cite{\@for\@citeb:=#2\do
    {\@citea\def\@citea{\@citedelim\penalty\@m\ }%
     \edef\@citeb{\expandafter\@iden\@citeb}%
     \if@filesw\immediate\write\@auxout{\string\citation{\@citeb}}\fi
     \@ifundefined{b@\@citeb}{{\reset@font\bfseries ?}%
       \G@refundefinedtrue\@latex@warning
       {Citation `\@citeb' on page \thepage \space undefined}}%
     {\csname b@\@citeb\endcsname}}}{#1}}
